@:ifFeature("anon_read.keyValueIterator", "dynamic_read.keyValueIterator")
class haxe.iterators.ArrayKeyValueIterator<T : haxe.iterators.ArrayKeyValueIterator.T> {

	public function new(array:Array<haxe.iterators.ArrayKeyValueIterator.T>) {
		this.current = 0;
		this.array = array;
	}

	@:value(0)
	var current:Int;

	var array:Array<haxe.iterators.ArrayKeyValueIterator.T>;

	public function hasNext() {
		return this.current < this.array.length;
	}

	public function next() {
		return {value : this.array[this.current], key : this.current ++};
	}
}